{
  "name": "eslint-plugin-sort-export-all",
  "version": "1.0.5",
  "description": "ESLint rule that helps sort export *",
  "repository": {
    "url": "https://github.com/nirtamir2/eslint-plugin-sort-export-all",
    "type": "git"
  },
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc",
    "test": "jest",
    "prettier": "prettier --write \"**/*.{ts,js,json}\"",
    "lint": "eslint --fix \"src/**/*.{ts,js,json}\"",
    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "README.md",
    "package.json",
    "lib"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Nir Tamir <nirtamir2@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/eslint": "^7.2.0",
    "@types/natural-compare": "^1.4.0",
    "@types/node": "^14.0.19",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "concurrently": "^5.2.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "2.21.2",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "typescript": "~3.9.6"
  },
  "dependencies": {
    "natural-compare": "^1.4.0"
  },
  "peerDependencies": {
    "eslint": "^4.0.0 || ^5.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(src/.*\\.test)\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.d\\.ts$",
      "lib/.*"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ]
  }
}
