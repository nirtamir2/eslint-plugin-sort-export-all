{
  "name": "eslint-plugin-sort-export-all",
  "type": "module",
  "version": "2.0.0",
  "packageManager": "pnpm@10.4.0",
  "description": "ESLint rule that helps sort export *",
  "author": "Nir Tamir <nirtamir2@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/nirtamir2",
  "homepage": "https://github.com/nirtamir2/eslint-plugin-sort-export-all#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirtamir2/eslint-plugin-sort-export-all.git"
  },
  "bugs": "https://github.com/nirtamir2/eslint-plugin-sort-export-all/issues",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "import"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config.d.mts",
      "default": "./dist/config.mjs"
    },
    "./types": {
      "types": "./dist/types.d.mts",
      "default": "./dist/types.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.mts",
        "./dist/*"
      ],
      "./config": [
        "./dist/config.d.mts"
      ],
      "./types": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "ci": "pnpm run --parallel --aggregate-output \"/^(lint|format|type-check).*/\"",
    "dev": "unbuild --stub",
    "format": "prettier \"**/*\" --write --ignore-unknown --log-level silent",
    "lint": "pnpm run dev && eslint . --fix",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "dependencies": {
    "natural-compare": "^1.4.0"
  },
  "devDependencies": {
    "@nirtamir2/eslint-config": "0.0.2-beta.20",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/eslint": "^9.6.1",
    "@types/natural-compare": "^1.4.3",
    "@types/node": "^22.13.1",
    "@typescript-eslint/typescript-estree": "^8.24.0",
    "@typescript-eslint/utils": "^8.24.0",
    "bumpp": "^10.0.2",
    "ci-info": "^4.1.0",
    "code-tag": "^1.2.0",
    "eslint": "^9.20.0",
    "eslint-define-config": "^2.1.0",
    "eslint-vitest-rule-tester": "^1.1.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.0",
    "prettier-plugin-embed": "^0.4.15",
    "prettier-plugin-packagejson": "^2.5.8",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
